{
  "supportedFormats": ["image/jpeg", "image/jpg", "image/png", "image/webp"],
  "maxFileSize": 52428800,
  "maxImageDimensions": {
    "width": 4096,
    "height": 4096
  },
  "exportFormats": [
    {
      "name": "JPEG",
      "extension": "jpg",
      "mimeType": "image/jpeg",
      "quality": true
    },
    {
      "name": "PNG",
      "extension": "png",
      "mimeType": "image/png",
      "quality": false
    },
    {
      "name": "WebP",
      "extension": "webp",
      "mimeType": "image/webp",
      "quality": true
    }
  ],
  "qualitySettings": [
    { "name": "Low", "value": 0.6 },
    { "name": "Medium", "value": 0.8 },
    { "name": "High", "value": 0.9 },
    { "name": "Maximum", "value": 1.0 }
  ],
  "historyLimit": 10,
  "canvasSettings": {
    "minZoom": 0.1,
    "maxZoom": 5.0,
    "zoomStep": 0.1,
    "backgroundColor": "#0F172A"
  }
}